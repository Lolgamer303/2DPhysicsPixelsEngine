{
  "version": 2,
  "builds": [
    {
      "src": "build/web/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.py",
      "use": "@vercel/static"
    },
    {
      "src": "*.apk",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ]
}